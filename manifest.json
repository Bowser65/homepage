{
  "manifest_version": 2,

  "name": "AlexFlipnote/Homepage",
  "description": "A website that gives you a clean homepage with random backgrounds!",
  "version": "0.0.2",

  "chrome_url_overrides": {
    "newtab": "index.html"
   },

  "icons": {
    "128": "assets/logo.png",
    "16": "assets/logo.png",
    "32": "assets/logo.png",
    "48": "assets/logo.png"
  },

  "browser_action": {
   "default_icon": "assets/logo.png",
   "default_popup": "window.html"
 },

  "permissions": [
    "activeTab"
  ]
}
